{
  "name": "<%= utils.sanitize(appname) %>",
  "license": "SEE LICENSE IN <your-license-filename>",
  "version": "0.0.0",
  "scripts": {
    "cap.add.ios": "npx cap add ios",
    "cap.add.android": "npx cap add android",
    "cap.ios": "npx cap open ios",
    "cap.android": "npx cap open android",
    "cap.copy": "npx cap copy",
    "cap.update": "npx cap update",
    "ngcc": "ngcc",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "dependencies": {
    "@angular/animations": "<%= angularVersion %>",
    "@angular/common": "<%= angularVersion %>",
    "@angular/compiler": "<%= angularVersion %>",
    "@angular/core": "<%= angularVersion %>",
    "@angular/forms": "<%= angularVersion %>",
    "@angular/platform-browser": "<%= angularVersion %>",
    "@angular/platform-browser-dynamic": "<%= angularVersion %>",
    "@angular/router": "<%= angularVersion %>",
    "@capacitor/android": "<%= capacitorVersion %>",
    "@capacitor/core": "<%= capacitorVersion %>",
    "@capacitor/ios": "<%= capacitorVersion %>",
    "@<%= npmScope %>/scss": "file:<%= pathOffset %>libs/scss",
    "@<%= npmScope %>/ionic-scss": "file:<%= pathOffset %>xplat/<%= xplatFolderName %>/scss",
    "@ionic-native/core": "<%= ionicNativeVersion %>",
    "@ionic-native/splash-screen": "<%= ionicNativeVersion %>",
    "@ionic-native/status-bar": "<%= ionicNativeVersion %>",
    "@ionic/angular": "<%= ionicAngularVersion %>",
    "@ngx-translate/core": "<%= ngxTranslateVersion %>",
    "@ngx-translate/http-loader": "<%= ngxTranslateHttpVersion %>",
    "rxjs": "<%= rxjsVersion %>",
    "zone.js": "<%= zonejsVersion %>"
  },
  "devDependencies": {
    "@capacitor/cli": "~1.5.1",
    "@angular-devkit/architect": "<%= angularDevkitVersion %>",
    "@angular-devkit/build-angular": "<%= angularDevkitVersion %>",
    "@angular-devkit/core": "<%= angularVersion %>",
    "@angular-devkit/schematics": "<%= angularVersion %>",
    "@angular/cli": "<%= angularVersion %>",
    "@angular/compiler": "<%= angularVersion %>",
    "@angular/compiler-cli": "<%= angularVersion %>",
    "@angular/language-service": "<%= angularVersion %>",
    "@ionic/angular-toolkit": "<%= ionicAngularToolkitVersion %>",
    "@types/jasmine": "file:<%= pathOffset %>node_modules/@types/jasmine",
    "@types/jasminewd2": "file:<%= pathOffset %>node_modules/@types/jasminewd2",
    "@types/node": "file:<%= pathOffset %>node_modules/@types/node",
    "codelyzer": "<%= codelyzerVersion %>",
    "jasmine-core": "file:<%= pathOffset %>node_modules/jasmine-core",
    "jasmine-spec-reporter": "file:<%= pathOffset %>node_modules/jasmine-spec-reporter",
    "karma": "file:<%= pathOffset %>node_modules/karma",
    "karma-chrome-launcher": "file:<%= pathOffset %>node_modules/karma-chrome-launcher",
    "karma-coverage-istanbul-reporter": "file:<%= pathOffset %>node_modules/karma-coverage-istanbul-reporter",
    "karma-jasmine": "file:<%= pathOffset %>node_modules/karma-jasmine",
    "karma-jasmine-html-reporter": "file:<%= pathOffset %>node_modules/karma-jasmine-html-reporter",
    "protractor": "file:<%= pathOffset %>node_modules/protractor",
    "ts-node": "file:<%= pathOffset %>node_modules/ts-node",
    "tslint": "file:<%= pathOffset %>node_modules/tslint",
    "typescript": "file:<%= pathOffset %>node_modules/typescript"
  }
}
